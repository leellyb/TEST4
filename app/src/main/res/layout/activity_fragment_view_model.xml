        android:layout_marginTop="80dp"
        app:layout_constraintEnd_toEndOf="@id/fragmentContainerView10"
        app:layout_constraintStart_toStartOf="@id/fragmentContainerView10"
        app:layout_constraintTop_toBottomOf="@id/fragmentContainerView10"
        tools:layout="@layout/fragment_model_b" />
